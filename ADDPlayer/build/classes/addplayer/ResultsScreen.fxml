<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="addplayer.ResultsScreenController">
   <children>
      <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" prefHeight="800.0" prefWidth="350.0" vbarPolicy="NEVER">
         <content>
            <AnchorPane fx:id="previewPane" prefHeight="800.0" prefWidth="350.0" />
         </content></ScrollPane>
      <VBox layoutX="500.0" layoutY="300.0" prefHeight="200.0" prefWidth="400.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="400.0" spacing="10.0">
               <children>
                  <Label text="Player:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="playerField" text="Player1">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="400.0" spacing="10.0">
               <children>
                  <Label text="Score:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="pointsField" text="0">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="400.0" spacing="10.0">
               <children>
                  <Button fx:id="playAgainButton" mnemonicParsing="false" onAction="#playAgainButtonAction" text="Play Again" />
                  <Button fx:id="mainMenuButton" mnemonicParsing="false" onAction="#mainMenuButtonAction" text="Main Menu" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
